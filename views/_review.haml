- requester = review.requester

%tr.review
  %td
    %strong= "<span class='req_name'>"+ requester.amzn_name + "</span>"
    %br/
    = "<span class='req_id'>"+ requester.amzn_id + "</span>"
  %td
    .attr_label comm:
    = review.comm_text
    %br/
    .attr_label fair:
    = review.fair_text
    %br/
    .attr_label fast:
    = review.fast_text
    %br/
    .attr_label pay:
    = review.pay_text
  %td
    = review.notes